<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="mobilelecture.cdp12_app">

    <application
        android:allowBackup="true"
        android:icon="@drawable/s_con"
        android:label="@string/app_name"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".MapViewActivity"
            android:label="@string/title_activity_map_view"></activity>
        <activity
            android:name=".UserInformActivity"
            android:label="@string/title_activity_user_inform"></activity>
        <!--
             ATTENTION: This was auto-generated to add Google Play services to your project for
             App Indexing.  See https://g.co/AppIndexing/AndroidStudio for more information.
        -->
        <meta-data
            android:name="com.google.android.gms.version"
            android:value="@integer/google_play_services_version" />

        <activity
            android:name=".CustomAdapter_listview_goods"
            android:label="@string/title_activity_custom_adapter_listview_goods"
            android:theme="@style/AppTheme.NoActionBar"></activity>
    </application>

</manifest>
